{
    "name": "construct",
    "build": {
        "context": "..",
        "dockerfile": "./Dockerfile"
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace/construct,type=bind",
    "workspaceFolder": "/workspace/construct",
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker:2": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go",
                "zxh404.vscode-proto3",
                "vitest.explorer",
                "bufbuild.vscode-buf",
                "ms-vscode.extension-test-runner",
                "GitHub.vscode-github-actions",
                "eamodio.gitlens"
            ]
        }
    }
}
